{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/cron",
      "destination": "/api/cron.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
